{
  "title": "TeamCache Core",
  "schemaVersion": 39,
  "version": 1,
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "graph",
      "title": "Active Connections",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "nginx_connections_active",
          "legendFormat": "{{instance}}"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Bond0 Network Throughput (Host)",
      "description": "Physical bond0 interface (2x10Gbps LACP) - matches iftop",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 111,
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "spanNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "unit": "bps",
          "decimals": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": [
            "mean",
            "max"
          ]
        }
      },
      "targets": [
        {
          "expr": "rate(node_network_receive_bytes_total{device=\"bond0\"}[1m]) * 8",
          "legendFormat": "bond0 RX",
          "refId": "A"
        },
        {
          "expr": "rate(node_network_transmit_bytes_total{device=\"bond0\"}[1m]) * 8",
          "legendFormat": "bond0 TX",
          "refId": "B"
        },
        {
          "expr": "(rate(node_network_receive_bytes_total{device=\"bond0\"}[1m]) + rate(node_network_transmit_bytes_total{device=\"bond0\"}[1m])) * 8",
          "legendFormat": "bond0 Total",
          "refId": "C"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Cache Performance",
      "description": "Cache hit ratio and request statistics",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 0,
        "y": 9
      },
      "id": 107,
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "yellow",
                "value": 30
              },
              {
                "color": "green",
                "value": 70
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100,
          "decimals": 1
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true,
        "text": {
          "titleSize": 16,
          "valueSize": 32
        },
        "orientation": "auto"
      },
      "targets": [
        {
          "expr": "nginx_cache_hit_ratio{node=\"all\"}",
          "legendFormat": "Hit Ratio",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Cache Statistics",
      "description": "Request counts over monitoring period",
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 9
      },
      "id": 109,
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "text",
                "value": null
              }
            ]
          },
          "unit": "short",
          "decimals": 0
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Cache HITs"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Cache MISSes"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "orange"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Total Requests"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "textMode": "value_and_name",
        "graphMode": "none",
        "colorMode": "value",
        "orientation": "horizontal",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "expr": "nginx_cache_hits_total{node=\"all\"}",
          "legendFormat": "Cache HITs",
          "refId": "A"
        },
        {
          "expr": "nginx_cache_misses_total{node=\"all\"}",
          "legendFormat": "Cache MISSes",
          "refId": "B"
        },
        {
          "expr": "nginx_cache_hits_total{node=\"all\"} + nginx_cache_misses_total{node=\"all\"}",
          "legendFormat": "Total Requests",
          "refId": "C"
        }
      ]
    },
    {
      "type": "gauge",
      "title": "Cache Disk Usage",
      "description": "Total cache disk usage across all nodes",
      "gridPos": {
        "h": 5,
        "w": 8,
        "x": 12,
        "y": 9
      },
      "id": 101,
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "percentage",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 85
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "showThresholdLabels": true,
        "showThresholdMarkers": true,
        "text": {}
      },
      "targets": [
        {
          "expr": "(sum(nginx_cache_disk_used_bytes{node=\"physical\",disk=~\"disk[0-9]\"}) / (1600 * 1024 * 1024 * 1024)) * 100",
          "legendFormat": "Cache Usage %",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Cache Storage",
      "description": "Current cache size vs total capacity",
      "gridPos": {
        "h": 5,
        "w": 4,
        "x": 20,
        "y": 9
      },
      "id": 102,
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "unit": "bytes",
          "decimals": 1,
          "color": {
            "mode": "fixed",
            "fixedColor": "green"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Used"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Total (1.6TB)"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "textMode": "value_and_name",
        "graphMode": "none",
        "colorMode": "background"
      },
      "targets": [
        {
          "expr": "sum(nginx_cache_disk_used_bytes{node=\"physical\",disk=~\"disk[0-9]\"})",
          "legendFormat": "Used",
          "refId": "A"
        },
        {
          "expr": "1600 * 1024 * 1024 * 1024",
          "legendFormat": "Total (1.6TB)",
          "refId": "B"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Cache Usage by Disk",
      "description": "Individual disk usage",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 14
      },
      "id": 104,
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": {
            "mode": "percentage",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 85
              }
            ]
          },
          "unit": "percent",
          "min": 0,
          "max": 100
        }
      },
      "options": {
        "displayMode": "gradient",
        "orientation": "horizontal",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        },
        "showUnfilled": true
      },
      "targets": [
        {
          "expr": "nginx_cache_disk_usage_percent{node=\"physical\"} or nginx_cache_disk_usage_percent{node=\"all\",disk=\"total\"}",
          "legendFormat": "{{disk}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Cache Growth Over Time",
      "description": "Cache disk usage trend",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 27
      },
      "id": 105,
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "spanNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "unit": "bytes",
          "decimals": 0
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": []
        }
      },
      "targets": [
        {
          "expr": "nginx_cache_disk_used_bytes{node=\"all\",disk=\"total\"}",
          "legendFormat": "Total Cache Used",
          "refId": "A"
        },
        {
          "expr": "nginx_cache_disk_total_bytes{node=\"all\",disk=\"total\"}",
          "legendFormat": "Total Capacity",
          "refId": "B"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Cache HIT/MISS Rate",
      "description": "Real-time cache performance from nginx logs",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 14
      },
      "id": 110,
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "opacity",
            "spanNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "normal",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "mappings": [],
          "unit": "reqps",
          "decimals": 0
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "HITs"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "MISSes"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": [
            "mean",
            "last"
          ]
        }
      },
      "targets": [
        {
          "expr": "rate(nginx_cache_hits_total[1m])",
          "legendFormat": "HITs",
          "refId": "A"
        },
        {
          "expr": "rate(nginx_cache_misses_total[1m])",
          "legendFormat": "MISSes",
          "refId": "B"
        }
      ]
    },
    {
      "type": "table",
      "title": "Cache Node Details",
      "description": "Per-node cache statistics",
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 0,
        "y": 22
      },
      "id": 106,
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "unit": "short"
        }
      },
      "options": {
        "showHeader": true,
        "footer": {
          "show": true,
          "reducer": [
            "sum"
          ],
          "fields": [],
          "enablePagination": false
        }
      },
      "targets": [
        {
          "expr": "nginx_cache_object_count > 0",
          "format": "table",
          "instant": true,
          "legendFormat": "{{node}}",
          "refId": "A"
        }
      ],
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "__name__": true,
              "job": true,
              "instance": true
            },
            "indexByName": {},
            "renameByName": {
              "node": "Cache Node",
              "Value": "Objects Cached"
            }
          }
        }
      ]
    }
  ]
}